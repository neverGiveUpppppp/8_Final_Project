
package com.fpj.trendeater.board.model.vo;

import java.sql.Date;

public class Board {

	private int boardId;
	private String boardTitle;
	private String boardContent;
	private Date boardCreateDate;
	private Date boardModifyDate;
	private int boardCount;
	private String boardStatus;
	private String admindId;
	
	public Board() {}

	public Board(int boardId, String boardTitle, String boardContent, Date boardCreateDate, Date boardModifyDate,
			int boardCount, String boardStatus, String admindId) {
		super();
		this.boardId = boardId;
		this.boardTitle = boardTitle;
		this.boardContent = boardContent;
		this.boardCreateDate = boardCreateDate;
		this.boardModifyDate = boardModifyDate;
		this.boardCount = boardCount;
		this.boardStatus = boardStatus;
		this.admindId = admindId;
	}

	@Override
	public String toString() {
		return "Board [boardId=" + boardId + ", boardTitle=" + boardTitle + ", boardContent=" + boardContent
				+ ", boardCreateDate=" + boardCreateDate + ", boardModifyDate=" + boardModifyDate + ", boardCount="
				+ boardCount + ", boardStatus=" + boardStatus + ", admindId=" + admindId + "]";
	}

	public Board(int boardId, String boardTitle, String boardContent, Date boardCreateDate, Date boardModifyDate,
			int boardCount, String boardStatus, String admindId) {
		super();
		this.boardId = boardId;
		this.boardTitle = boardTitle;
		this.boardContent = boardContent;
		this.boardCreateDate = boardCreateDate;
		this.boardModifyDate = boardModifyDate;
		this.boardCount = boardCount;
		this.boardStatus = boardStatus;
		this.admindId = admindId;
	}

	
	
	
	
	
}

